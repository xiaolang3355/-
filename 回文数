class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        s = str(x)
        l = 0
        r = len(s)-1
        if x<0 or(x%10 == 0 and x != 0):
            return False
        while l<r:
            if s[l] != s[r]:
                return False
            l += 1
            r -= 1
                
        return True
