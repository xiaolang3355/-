class Solution(object):
    def maxArea(self, height):
        """
        :type height: List[int]
        :rtype: int
        """
        l,r=0,len(height)-1
        tank = 0
        while l<r:
            current_area = (r-l)*min(height[l],height[r])
            tank = max(tank,current_area)
            if height[l] < height[r]:
                l += 1
            else:
                r -=1
        return tank
